    "url": "https://github.com/rvantonisse/rv-antonisse.nl/issues"
  },
  "homepage": "https://rv-antonisse.nl",
  "devDependencies": {
    "@11ty/eleventy": "^0.11.0",
    "@svrx/cli": "^1.1.2",
    "autoprefixer": "^10.0.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.1.1",
    "postcss-cli": "^8.1.0",
    "postcss-import": "^12.0.1",
    "postcss-preset-env": "^6.7.0"
  }
